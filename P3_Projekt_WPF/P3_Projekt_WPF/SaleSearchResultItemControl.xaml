<UserControl x:Class="P3_Projekt_WPF.SaleSearchResultItemControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:P3_Projekt_WPF"
             mc:Ignorable="d" 
             d:DesignHeight="70" d:DesignWidth="300" Width="auto">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
                <Image x:Name="image_SaleProductSearchItemImage" Margin="0,0,0,0" MaxHeight="70" MaxWidth="120" Grid.Column="1" />
                <Label x:Name="label_SaleProductSearchItemText" Content="Label" Margin="0,0,0,0" Grid.Column="0"/>

    </Grid>
</UserControl>
